
public class Typedataandvariable {

    public static void main(String[] args) {
        int value;
        value = 80;
        String name = "Hanas";
        
        float decimal = 3.14f;
        double decimal2 = 4.56644;
        
        boolean GenderType = true;
        boolean abc = false;   
        
        System.out.println(value);
        System.out.println(name);
        
        int nilai = 70;
        
        System.out.println(nilai);
        
        System.out.println(decimal);
        System.out.println(decimal2);
    }
}

public class TypeDataofString {
    public static void main(String[] args) {
        
        char name1 = 'H';
        String name = "    Hanas";
        String student = " Hanas Bayu Pratama";
        
        int a = 10;
        String nama = "10";
        
        System.out.println(name);
        System.out.println(a+nama);
        System.out.println("my name is "+"Hanas");
        System.out.println("The Student of CIIT is"+student);
    }
}

public class KonversiTypeofData {

    public static void main(String[] args) {
        
        String number = "120";
        String value = "200";
        
        int number2 = Integer.valueOf(number);
        Double value2 = Double.parseDouble(value);
        
        System.out.println(number2+80);
        System.out.println(value2+40);
        
    }
}

public class Compare {


    public static void main(String[] args) {
        int a = 20;
        int b = 20;
        int c = 12;
        int d = 31;
        
        boolean result = a == b; //same
        boolean outcome = a != b; //no same
        boolean isgreaterthan = a > b;//is greater than
        boolean islessthan = c < d;//is less than
        boolean isgreaterthanorequal = b >= d;//is greater than or equal
        boolean islessthanorequal = d <= a;//is less than or equal
        
        
        System.out.println(result);
        System.out.println(outcome);
        System.out.println(isgreaterthan);
        System.out.println(islessthan);
        System.out.println(isgreaterthanorequal);
        System.out.println(islessthanorequal);
    }
    
}


public class Arithmetic {

    
    public static void main(String[] args) {
        
        int plus = 90 + 50;
        System.out.println(plus);
        
        int minus = 50 - 70;
        System.out.println(minus);
        
        int A = 4;
        int B = 12;
        int multipliedby = A * B;
        int dividedby = B / A;
        System.out.println(multipliedby);
        System.out.println(dividedby);
        
        int C = 23;
        int D = 6;
        int Percent = C % D;
        System.out.println(Percent);
    }
    
}



